{
  "name": "fp-frontend",
  "description": "Klient for FPSAK",
  "main": "./dist/index.js",
  "author": "NAV",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/fp-frontend.git"
  },
  "scripts": {
    "test": "lerna run test --stream",
    "test:changed": "lerna run test --stream --since origin/master",
    "test:coverage": "lerna run test:coverage --stream",
    "tsc": "lerna run tsc --stream",
    "tsc:changed": "lerna run tsc --stream --since origin/master",
    "eslint": "lerna run eslint --stream",
    "eslint:changed": "lerna run eslint --stream --since origin/master",
    "stylelint": "lerna run stylelint --stream",
    "prettier": "lerna run prettier --stream",
    "clean": "lerna run clean --stream",
    "dev": "node -e \"console.log('For å få oppdateringer i pakke, skriv: yarn dev --scope=@navikt/fp-{pakkenavn}')\" && yarn lerna run dev --scope=@navikt/fp-frontend-app",
    "build": "lerna run build --stream",
    "doc": "react-asciidoc-generator ./packages/ -o ../../docs/generated/react.adoc --handlebar-template src/docs/asciidoc-template.handlebars",
    "up": "docker-compose up -d --remove-orphans --build",
    "down": "docker-compose down",
    "remove-node-modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "storybook-clean-deploy-folder": "rm -rf ./.storybook-static-build",
    "storybook-build-all": "lerna run build-storybook --stream",
    "storybook-create-deploy-folder": "node scripts/create-storybook-index.js",
    "storybook-deploy-to-github": "gh-pages -d .storybook-static-build -t true",
    "deploy-storybook": "yarn storybook-clean-deploy-folder && yarn storybook-build-all && yarn storybook-create-deploy-folder && yarn storybook-deploy-to-github",
    "sentry-release": "(SENTRY_RELEASE=$(git rev-parse --short HEAD); node ./scripts/sentry-release)",
    "prepare": "husky install"
  },
  "workspaces": [
    "apps/**/*",
    "packages/**/*"
  ],
  "dependencies": {
    "dotenv": "16.3.1"
  },
  "devDependencies": {
    "@navikt/aksel-stylelint": "4.9.0",
    "@sentry/cli": "2.20.4",
    "@storybook/addon-essentials": "7.2.1",
    "@storybook/addon-links": "7.2.1",
    "@storybook/builder-vite": "7.2.1",
    "@storybook/cli": "7.2.1",
    "@storybook/react": "7.2.1",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.2.19",
    "@types/react-collapse": "5.0.1",
    "@types/react-dom": "18.2.7",
    "@types/react-responsive": "8.0.5",
    "@types/react-router-dom": "5.3.3",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "@vitejs/plugin-react": "4.0.3",
    "@vitest/coverage-c8": "0.33.0",
    "axios-mock-adapter": "1.21.5",
    "cross-env": "7.0.3",
    "eslint": "8.46.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.9.0",
    "eslint-import-resolver-lerna": "2.0.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-testing-library": "5.11.0",
    "eslint-plugin-vitest": "0.2.8",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "lerna": "7.1.4",
    "lint-staged": "13.2.3",
    "react-test-renderer": "18.2.0",
    "shelljs": "0.8.5",
    "stylelint": "15.10.2",
    "stylelint-order": "6.0.3",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vite-plugin-dts": "2.3.0",
    "vite-plugin-externalize-deps": "0.7.0",
    "vite-plugin-html": "3.2.0",
    "vitest": "0.33.0"
  },
  "resolutions": {
    "decode-uri-component": "0.2.1",
    "esquery": "1.4.0",
    "glob-parent": "5.1.2",
    "got": "11.8.5",
    "json5": "2.2.3",
    "parse-url": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "trim": "0.0.3",
    "trim-newlines": "3.0.1"
  },
  "lint-staged": {
    "apps/**/*.{ts,tsx,js,jsx, css}": "prettier --write",
    "packages/**/*.{ts,tsx,js,jsx, css}": "prettier --write"
  },
  "version": "0.0.0",
  "packageManager": "yarn@3.4.1"
}
