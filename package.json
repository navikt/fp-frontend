{
  "name": "fp-frontend",
  "description": "Klient for FPSAK",
  "main": "./dist/index.js",
  "author": "Nav",
  "private": true,
  "type": "module",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/fp-frontend.git"
  },
  "scripts": {
    "test": "turbo run test --concurrency=5",
    "tsc": "turbo run tsc",
    "lint": "turbo run stylelint eslint",
    "ci": "turbo run build test",
    "eslint": "turbo run eslint",
    "eslint:fix": "turbo run eslint:fix",
    "stylelint": "turbo run stylelint",
    "prettier": "turbo run prettier",
    "clean": "turbo run clean",
    "dev": "turbo run dev --filter=@navikt/fp-frontend",
    "dev-avdelingsleder": "turbo run dev --filter=@navikt/fp-avdelingsleder",
    "dev-journalforing": "turbo run dev --filter=@navikt/fp-journalforing",
    "build": "turbo run build",
    "up": "docker-compose up -d --remove-orphans --build",
    "down": "docker-compose down",
    "remove-node-modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "build:storybook": "turbo run build-storybook && yarn storybook-create-deploy-folder",
    "storybook-create-deploy-folder": "node scripts/create-storybook-index.cjs",
    "prepare": "husky install",
    "update-swagger": "node scripts/hent-openapi-spec.js",
    "generate:local": "yarn update-swagger lokal && openapi-ts",
    "generate": "yarn update-swagger && openapi-ts"
  },
  "workspaces": [
    "apps/**/*",
    "packages/**/*"
  ],
  "devDependencies": {
    "@hey-api/openapi-ts": "0.88.0",
    "@navikt/aksel-stylelint": "7.33.5",
    "@storybook/addon-a11y": "10.0.8",
    "@storybook/addon-links": "10.0.8",
    "@storybook/react": "10.0.2",
    "@testing-library/jest-dom": "6.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/react-responsive": "9.0.0",
    "@types/react-router-dom": "5.3.3",
    "husky": "9.1.7",
    "jsdom": "27.2.0",
    "knip": "^5.70.1",
    "lint-staged": "16.2.7",
    "prettier": "3.6.2",
    "shelljs": "0.10.0",
    "stylelint": "16.25.0",
    "stylelint-order": "7.0.0",
    "turbo": "2.6.1",
    "typescript": "5.9.3",
    "vitest": "4.0.6",
    "vitest-canvas-mock": "0.3.3"
  },
  "resolutions": {
    "@adobe/css-tools": "4.3.2",
    "@types/react": "19.1.2",
    "axios": "1.11.0",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "lint-staged": {
    "apps/**/*.{ts,tsx,js,jsx, css}": "prettier --write",
    "packages/**/*.{ts,tsx,js,jsx, css}": "prettier --write"
  },
  "version": "0.0.0",
  "packageManager": "yarn@3.4.1",
  "msw": {
    "workerDirectory": [
      "./.storybook/mock"
    ]
  }
}
