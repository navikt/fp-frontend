{
  "name": "fp-frontend",
  "description": "Klient for FPSAK",
  "main": "./dist/index.js",
  "author": "NAV",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/fp-frontend.git"
  },
  "scripts": {
    "test": "jest -o --colors",
    "test:all": "jest --colors",
    "test:all:testonly": "jest --colors --selectProjects test",
    "test:watch": "jest --watch --colors",
    "test:watch:all": "jest --watchAll --colors",
    "test:coverage": "jest --coverage --colors --selectProjects test",
    "ts-check": "tsc  --pretty",
    "lint": "eslint --cache \"packages/**/*.ts*\"",
    "lint:fix": "eslint --fix --cache \"packages/**/*.ts*\"",
    "less:lint": "stylelint \"packages/**/*.less\"",
    "dev": "cross-env NODE_ENV=development node webpack/webpack.dev.server.js --config webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "lerna": "lerna",
    "lerna:publish": "(lerna updated && lerna publish --cd-version patch --yes -m \"publish [ci skip]\") || echo \"Ingen endringer avslutter OK\"",
    "doc": "react-asciidoc-generator ./packages/ -o ../../docs/generated/react.adoc --handlebar-template src/docs/asciidoc-template.handlebars",
    "up": "docker-compose up -d --remove-orphans --build",
    "down": "docker-compose down",
    "remove-node-modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "lint:config": "node -e 'console.dir(require(\"./.eslintrc.js\"))'",
    "storybook": "start-storybook -p 9002",
    "deploy-storybook": "build-storybook -o .static_storybook && storybook-to-ghpages --existing-output-dir=.static_storybook"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/runtime": "^7.15.4",
