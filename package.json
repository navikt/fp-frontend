{
  "name": "fp-frontend",
  "description": "Klient for FPSAK",
  "main": "./dist/index.js",
  "author": "NAV",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/fp-frontend.git"
  },
  "scripts": {
    "test": "lerna run test --stream",
    "test:changed": "lerna run test --stream --since origin/master",
    "test:coverage": "lerna run test:coverage --stream",
    "tsc": "lerna run tsc --stream",
    "tsc:changed": "lerna run tsc --stream --since origin/master",
    "eslint": "lerna run eslint --stream",
    "eslint:changed": "lerna run eslint --stream --since origin/master",
    "stylelint": "lerna run stylelint --stream",
    "clean": "lerna run clean --stream",
    "dev": "node -e \"console.log('For å få oppdateringer i pakke, skriv: yarn dev --scope=@navikt/fp-{pakkenavn}')\" && yarn lerna run dev --scope=@navikt/fp-frontend-app",
    "build": "lerna run build --stream",
    "doc": "react-asciidoc-generator ./packages/ -o ../../docs/generated/react.adoc --handlebar-template src/docs/asciidoc-template.handlebars",
    "up": "docker-compose up -d --remove-orphans --build",
    "down": "docker-compose down",
    "remove-node-modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "storybook-clean-deploy-folder": "rm -rf ./.storybook-static-build",
    "storybook-build-all": "lerna run build-storybook --stream",
    "storybook-create-deploy-folder": "node scripts/create-storybook-index.js",
    "storybook-temp-replace": "node scripts/storybook-temp-replace.js",
    "storybook-deploy-to-github": "gh-pages -d .storybook-static-build",
    "deploy-storybook": "yarn storybook-clean-deploy-folder && yarn storybook-build-all && yarn storybook-create-deploy-folder && yarn storybook-temp-replace && yarn storybook-deploy-to-github",
    "sentry-release": "(SENTRY_RELEASE=$(git rev-parse --short HEAD); node ./scripts/sentry-release)"
  },
  "workspaces": [
    "apps/**/*",
    "packages/**/*"
  ],
  "dependencies": {
    "@babel/runtime": "7.21.0",
    "dotenv": "16.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/eslint-parser": "7.21.3",
    "@babel/helper-regex": "7.10.5",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.0",
    "@babel/register": "7.21.0",
    "@navikt/ds-codemod": "2.8.4",
    "@sentry/cli": "2.16.0",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/builder-vite": "0.4.2",
    "@storybook/cli": "6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@storybook/theming": "6.5.16",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.29",
    "@types/react-collapse": "5.0.1",
    "@types/react-dom": "18.0.11",
    "@types/react-responsive": "8.0.5",
    "@types/react-router-dom": "5.3.3",
    "@types/sinon": "10.0.13",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-c8": "0.29.7",
    "axios-mock-adapter": "1.21.3",
    "babel-loader": "9.1.2",
    "body-parser": "1.20.2",
    "cross-env": "7.0.3",
    "eslint": "8.37.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-import-resolver-lerna": "2.0.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-testing-library": "5.10.2",
    "eslint-plugin-vitest": "0.0.57",
    "gh-pages": "5.0.0",
    "jsdom": "21.1.1",
    "lerna": "6.6.1",
    "raf": "3.4.1",
    "react-test-renderer": "18.2.0",
    "replace-in-files": "3.0.0",
    "source-map-loader": "4.0.1",
    "stylelint": "15.3.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.0.2",
    "vite": "4.2.1",
    "vite-plugin-dts": "2.1.0",
    "vite-plugin-externalize-deps": "0.5.0",
    "vite-plugin-html": "3.2.0",
    "vitest": "0.29.7"
  },
  "resolutions": {
    "decode-uri-component": "0.2.1",
    "esquery": "1.4.0",
    "glob-parent": "5.1.2",
    "got": "11.8.5",
    "json5": "2.2.3",
    "parse-url": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "trim": "0.0.3",
    "trim-newlines": "3.0.1"
  },
  "version": "0.0.0",
  "packageManager": "yarn@3.4.1"
}
