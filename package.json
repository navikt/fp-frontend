{
  "name": "fp-frontend",
  "description": "Klient for FPSAK",
  "main": "./dist/index.js",
  "author": "NAV",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/fp-frontend.git"
  },
  "scripts": {
    "test": "cross-env NODE_ICU_DATA=./node_modules/full-icu mochapack --opts mochapack.opts --include ./packages/utils-test/src/setup-mocha.ts --include ./packages/utils-test/src/setup-mocha-hook.ts --colors --webpack-config webpack/webpack.test.js \"packages/**/*.spec.*\"",
    "test:watch": "cross-env NODE_ICU_DATA=./node_modules/full-icu mochapack --opts mochapack.opts --include ./packages/utils-test/src/setup-mocha.ts --include ./packages/utils-test/src/setup-mocha-hook.ts  --watch --webpack-config webpack/webpack.test.watch.js \"packages/**/*.spec.*\"",
    "test:coverage": "cross-env NODE_ICU_DATA=./node_modules/full-icu nyc mochapack --opts mochapack.opts --include ./packages/utils-test/src/setup-mocha.ts --include ./packages/utils-test/src/setup-mocha-hook.ts  --webpack-config webpack/webpack.test.coverage.js \"packages/**/*.spec.*\"",
    "dev": "cross-env NODE_ENV=development NODE_ICU_DATA=./node_modules/full-icu node webpack/webpack.dev.server.js --config webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --no-progress --colors --config webpack/webpack.prod.js",
    "lerna": "lerna",
    "lerna:publish": "(lerna updated && lerna publish --cd-version patch --yes -m \"publish [ci skip]\") || echo \"Ingen endringer avslutter OK\"",
    "less:lint": "stylelint \"packages/**/*.less\"",
    "ts-check": "tsc --pretty",
    "doc": "react-asciidoc-generator ./packages/ -o ../../docs/generated/react.adoc --handlebar-template src/docs/asciidoc-template.handlebars",
    "up": "docker-compose up -d --remove-orphans --build",
    "down": "docker-compose down",
    "lint": "./node_modules/.bin/eslint ./packages/**/*.ts*",
    "remove-node-modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "lint:config": "node -e 'console.dir(require(\"./.eslintrc.js\"))'",
    "storybook": "start-storybook -p 9001",
    "deploy-storybook": "build-storybook -o .static_storybook && storybook-to-ghpages --existing-output-dir=.static_storybook"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "axios": "^0.21.0",
    "core-js": "^3.8.1",
    "dotenv": "^8.0.0",
    "history": "^4.9.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-modal": "3.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.10",
    "@commercetools/enzyme-extensions": "^5.0.0",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-docs": "^6.1.10",
    "@storybook/addon-knobs": "^6.1.10",
    "@storybook/addons": "^6.1.10",
    "@storybook/cli": "^6.1.10",
    "@storybook/react": "^6.1.10",
    "@storybook/storybook-deployer": "^2.8.7",
    "@storybook/theming": "^6.1.10",
    "@svgr/webpack": "^5.5.0",
    "@types/chai": "^4.2.14",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.8",
    "@types/mocha": "^8.2.0",
    "@types/react": "^16.14.2",
    "@types/react-collapse": "^5.0.0",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-form": "^8.3.0",
    "@types/sinon": "^9.0.9",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "body-parser": "*",
    "cache-loader": "^4.1.0",
    "chai": "^4.2.0",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^6.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-lerna": "^2.0.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "full-icu": "^1.3.1",
    "html-webpack-plugin": "^4.5.0",
    "ignore-styles": "^5.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^16.4.0",
    "json-loader": "^0.5.7",
    "lerna": "^3.22.1",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "mini-css-extract-plugin": "^1.3.2",
    "mocha": "^8.2.1",
    "mochapack": "^2.0.6",
    "null-loader": "^4.0.1",
    "nyc": "^15.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "react-test-renderer": "^16.14.0",
    "redux-mock-store": "^1.5.4",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-order": "^4.1.0",
    "terser-webpack-plugin": "^4.2.3",
    "thread-loader": "^3.0.1",
    "typescript": "4.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.5.0"
  },
  "resolutions": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0"
  },
  "nyc": {
    "branches": 40,
    "lines": 60,
    "functions": 50,
    "statements": 50,
    "cache": false,
    "check-coverage": true,
    "require": [
      "@babel/register",
      "./setup/setup.js"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "text",
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".tsx",
      ".ts",
      ".es6"
    ],
    "exclude": [
      "packages/**/*.spec.tsx",
      "packages/**/*.spec.ts",
      "packages/storybook/**",
      "packages/utils-test/**"
    ]
  }
}
