{
  "name": "fp-frontend",
  "description": "Klient for FPSAK",
  "main": "./dist/index.js",
  "author": "Nav",
  "private": true,
  "type": "module",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/fp-frontend.git"
  },
  "scripts": {
    "test": "turbo run test --concurrency=1",
    "tsc": "turbo run tsc",
    "lint": "turbo run lint",
    "eslint": "turbo run eslint",
    "eslint:fix": "turbo run eslint:fix",
    "stylelint": "turbo run stylelint",
    "prettier": "turbo run prettier",
    "clean": "turbo run clean",
    "dev": "turbo run dev --filter=@navikt/fp-frontend",
    "dev-avdelingsleder": "turbo run dev --filter=@navikt/fp-avdelingsleder",
    "dev-journalforing": "turbo run dev --filter=@navikt/fp-journalforing",
    "build": "turbo run build",
    "up": "docker-compose up -d --remove-orphans --build",
    "down": "docker-compose down",
    "remove-node-modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
    "storybook-clean-deploy-folder": "rm -rf ./.storybook-static-build",
    "storybook-build-all": "turbo run build-storybook",
    "storybook-create-deploy-folder": "node scripts/create-storybook-index.cjs",
    "storybook-deploy-to-github": "gh-pages -d .storybook-static-build -t true",
    "deploy-storybook": "yarn storybook-clean-deploy-folder && yarn storybook-build-all && yarn storybook-create-deploy-folder && yarn storybook-deploy-to-github",
    "sentry-release": "(SENTRY_RELEASE=$(git rev-parse --short HEAD); node ./scripts/sentry-release.cjs)",
    "prepare": "husky install",
    "generate-types": "node ./scripts/generate-types.js"
  },
  "workspaces": [
    "apps/**/*",
    "packages/**/*"
  ],
  "devDependencies": {
    "@hey-api/openapi-ts": "0.85.2",
    "@navikt/aksel-stylelint": "7.32.1",
    "@sentry/cli": "2.56.1",
    "@storybook/addon-a11y": "9.1.10",
    "@storybook/addon-links": "9.1.10",
    "@storybook/react": "9.1.12",
    "@testing-library/jest-dom": "6.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/react-responsive": "9.0.0",
    "@types/react-router-dom": "5.3.3",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "jsdom": "27.0.0",
    "knip": "^5.65.0",
    "lint-staged": "16.2.4",
    "prettier": "3.6.2",
    "shelljs": "0.10.0",
    "stylelint": "16.25.0",
    "stylelint-order": "7.0.0",
    "turbo": "2.5.8",
    "typescript": "5.9.3",
    "vitest": "3.2.4",
    "vitest-canvas-mock": "0.3.3"
  },
  "resolutions": {
    "@adobe/css-tools": "4.3.2",
    "@types/react": "19.1.2",
    "axios": "1.11.0",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "lint-staged": {
    "apps/**/*.{ts,tsx,js,jsx, css}": "prettier --write",
    "packages/**/*.{ts,tsx,js,jsx, css}": "prettier --write"
  },
  "version": "0.0.0",
  "packageManager": "yarn@3.4.1",
  "msw": {
    "workerDirectory": [
      "./.storybook/mock"
    ]
  }
}
