{
  "app": "fp-journalforing",
  "ingresses": [
    "https://fpjournalforing.intern.nav.no"
  ],
  "minReplicas": "2",
  "maxReplicas": "4",
  "limits": {
    "cpu": "1000m",
    "mem": "256Mi"
  },
  "requests": {
    "cpu": "20m",
    "mem": "120Mi"
  },
  "env": "production",
  "groups": [
    {
      "uuid": "73107205-17ec-4a07-a56e-e0a8542f90c9",
      "kommentar": "saksbehandler"
    }
  ],
  "outboundRules": [
    {
      "application": "fpmottak"
    }
  ],
  "externals": [
    "fpsak-api.prod-fss-pub.nais.io"
  ],
  "proxyRedirects": [
    {
      "path": "/fpsak/api",
      "url": "https://fpsak-api.prod-fss-pub.nais.io",
      "scope": "api://prod-fss.teamforeldrepenger.fpsak/.default"
    },
    {
      "path": "/fpmottak/api",
      "url": "http://fpmottak",
      "scope": "api://prod-gcp.teamforeldrepenger.fpmottak/.default"
    }
  ]
}
