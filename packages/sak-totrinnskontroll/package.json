{
  "name": "@navikt/fp-sak-totrinnskontroll",
  "description": "Komponent som lar beslutter godkjenne eller avslå vedtak gjort av en saksbehandler. Viser også beslutters valg til saksbehandler (readonly).",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --mode development --watch",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./coverage ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7067"
  },
  "dependencies": {
    "@navikt/ds-react": "4.4.0",
    "@navikt/fp-kodeverk": "1.0.0",
    "@navikt/fp-konstanter": "1.0.0",
    "@navikt/fp-types": "1.0.0",
    "@navikt/fp-types-avklar-aksjonspunkter": "1.0.0",
    "@navikt/ft-fakta-beregning": "4.2.25",
    "@navikt/ft-fakta-fordel-beregningsgrunnlag": "7.2.29",
    "@navikt/ft-form-hooks": "4.1.2",
    "@navikt/ft-form-validators": "2.0.34",
    "@navikt/ft-kodeverk": "2.1.14",
    "@navikt/ft-prosess-beregningsgrunnlag": "6.2.26",
    "@navikt/ft-types": "2.1.16",
    "@navikt/ft-ui-komponenter": "2.0.37",
    "@navikt/ft-utils": "2.0.33",
    "history": "5.3.0",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-hook-form": "7.45.0",
    "react-intl": "6.4.4",
    "react-router-dom": "6.14.0"
  },
  "devDependencies": {
    "@navikt/ds-css": "4.4.0",
    "@navikt/fp-storybook-utils": "1.0.0",
    "@navikt/fp-utils-test": "1.0.0",
    "@storybook/addon-actions": "7.0.23",
    "@storybook/react": "7.0.23",
    "@storybook/react-vite": "7.0.23",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.43.0",
    "storybook": "7.0.24",
    "stylelint": "15.9.0",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vitest": "0.32.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}
