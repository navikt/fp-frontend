{
  "name": "@navikt/fp-sak-totrinnskontroll",
  "description": "Komponent som lar beslutter godkjenne eller avslå vedtak gjort av en saksbehandler. Viser også beslutters valg til saksbehandler (readonly).",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "dev": "vite build --mode development --watch",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook -o .storybook-static-build",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook --quiet -p 7067"
  },
  "dependencies": {
    "@navikt/ds-react": "2.8.4",
    "@navikt/fp-kodeverk": "1.0.0",
    "@navikt/fp-konstanter": "1.0.0",
    "@navikt/fp-types": "1.0.0",
    "@navikt/fp-types-avklar-aksjonspunkter": "1.0.0",
    "@navikt/ft-fakta-beregning": "4.1.8",
    "@navikt/ft-fakta-fordel-beregningsgrunnlag": "7.0.11",
    "@navikt/ft-form-hooks": "4.0.11",
    "@navikt/ft-form-validators": "2.0.10",
    "@navikt/ft-kodeverk": "2.0.8",
    "@navikt/ft-prosess-beregningsgrunnlag": "6.0.13",
    "@navikt/ft-types": "2.0.8",
    "@navikt/ft-ui-komponenter": "2.0.9",
    "@navikt/ft-utils": "2.0.9",
    "history": "5.3.0",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-hook-form": "7.43.8",
    "react-intl": "6.3.2",
    "react-router-dom": "6.9.0"
  },
  "devDependencies": {
    "@navikt/ds-css": "2.8.4",
    "@navikt/fp-storybook-utils": "1.0.0",
    "@navikt/fp-utils-test": "1.0.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/builder-vite": "0.4.2",
    "@storybook/cli": "6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/storybook-deployer": "2.8.16",
    "@storybook/testing-react": "1.3.0",
    "@storybook/theming": "6.5.16",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "stylelint": "15.3.0",
    "typescript": "5.0.3",
    "vite": "4.2.1",
    "vitest": "0.29.7"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}
