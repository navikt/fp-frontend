{
  "name": "@navikt/fp-utils-test",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly -t es5 --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/fp-rest-api": "1.0.0",
    "@navikt/fp-rest-api-hooks": "1.0.0",
    "@testing-library/jest-dom": "5.16.5",
    "@navikt/ft-ui-komponenter": "1.3.15",
    "axios-mock-adapter": "1.21.2",
    "react": "18.2.0",
    "react-intl": "6.2.8"
  },
  "exports": {
    ".": "./dist/index.js"
  }
}
