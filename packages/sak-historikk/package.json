{
  "name": "@navikt/fp-sak-historikk",
  "description": "Komponent som viser historikken til en fagsak",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --mode development --watch",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7060"
  },
  "dependencies": {
    "@navikt/aksel-icons": "4.0.0",
    "@navikt/ds-react": "4.0.0",
    "@navikt/fp-kodeverk": "1.0.0",
    "@navikt/fp-konstanter": "1.0.0",
    "@navikt/fp-storybook-utils": "1.0.0",
    "@navikt/fp-types": "1.0.0",
    "@navikt/ft-fakta-beregning": "4.2.20",
    "@navikt/ft-kodeverk": "2.1.12",
    "@navikt/ft-prosess-beregningsgrunnlag": "6.2.20",
    "@navikt/ft-types": "2.1.14",
    "@navikt/ft-ui-komponenter": "2.0.34",
    "@navikt/ft-utils": "2.0.30",
    "classnames": "2.3.2",
    "history": "5.3.0",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-intl": "6.4.3",
    "react-router-dom": "6.12.0"
  },
  "devDependencies": {
    "@navikt/ds-css": "4.0.0",
    "@navikt/fp-storybook-utils": "1.0.0",
    "@navikt/fp-utils-test": "1.0.0",
    "@storybook/addon-actions": "7.0.20",
    "@storybook/react": "7.0.20",
    "@storybook/react-vite": "7.0.20",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.42.0",
    "storybook": "7.0.20",
    "stylelint": "15.7.0",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vitest": "0.32.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}
