{
  "name": "@navikt/fp-sak-historikk",
  "description": "Komponent som viser historikken til en fagsak",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --mode development --watch",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7060"
  },
  "dependencies": {
    "@navikt/aksel-icons": "3.1.2",
    "@navikt/ds-react": "3.1.2",
    "@navikt/fp-kodeverk": "1.0.0",
    "@navikt/fp-konstanter": "1.0.0",
    "@navikt/fp-storybook-utils": "1.0.0",
    "@navikt/fp-types": "1.0.0",
    "@navikt/ft-fakta-beregning": "4.2.13",
    "@navikt/ft-kodeverk": "2.1.7",
    "@navikt/ft-prosess-beregningsgrunnlag": "6.2.11",
    "@navikt/ft-types": "2.1.8",
    "@navikt/ft-ui-komponenter": "2.0.27",
    "@navikt/ft-utils": "2.0.24",
    "classnames": "2.3.2",
    "history": "5.3.0",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-intl": "6.4.2",
    "react-router-dom": "6.11.1"
  },
  "devDependencies": {
    "@navikt/ds-css": "3.1.2",
    "@navikt/fp-storybook-utils": "1.0.0",
    "@navikt/fp-utils-test": "1.0.0",
    "@storybook/addon-actions": "7.0.11",
    "@storybook/react": "7.0.11",
    "@storybook/react-vite": "7.0.11",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.40.0",
    "storybook": "7.0.11",
    "stylelint": "15.6.1",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "vitest": "0.31.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}
