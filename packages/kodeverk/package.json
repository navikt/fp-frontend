{
  "name": "@navikt/fp-kodeverk",
  "version": "1.0.0",
  "module": "index.jsx",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "dev": "webpack --env watch=true --config webpack.config.js",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "exports": {
    ".": "./dist/index.js"
  }
}
