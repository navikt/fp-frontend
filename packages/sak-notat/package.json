{
  "name": "@navikt/fp-sak-notat",
  "description": "Komponent som lar saksbehandlere skrive notater til andre saksbehandlere",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --mode development --watch",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7066"
  },
  "dependencies": {
    "@navikt/ds-react": "4.5.0",
    "@navikt/fp-kodeverk": "1.0.0",
    "@navikt/fp-types": "1.0.0",
    "@navikt/ft-form-hooks": "4.1.3",
    "@navikt/ft-form-validators": "2.0.35",
    "@navikt/ft-kodeverk": "2.1.15",
    "@navikt/ft-types": "2.1.17",
    "@navikt/ft-ui-komponenter": "2.0.38",
    "@navikt/ft-utils": "2.0.34",
    "dayjs": "1.11.9",
    "react": "18.2.0",
    "react-hook-form": "7.45.1",
    "react-intl": "6.4.4"
  },
  "devDependencies": {
    "@navikt/ds-css": "4.5.0",
    "@navikt/fp-storybook-utils": "1.0.0",
    "@storybook/addon-actions": "7.0.26",
    "@storybook/react": "7.0.24",
    "@storybook/react-vite": "7.0.24",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.44.0",
    "storybook": "7.0.24",
    "stylelint": "15.9.0",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vitest": "0.32.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}
