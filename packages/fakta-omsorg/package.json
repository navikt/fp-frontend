{
  "name": "@navikt/fp-fakta-omsorg",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --mode development --watch",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./coverage ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7009"
  },
  "dependencies": {
    "@navikt/ds-css": "4.7.2",
    "@navikt/ds-react": "4.7.2",
    "@navikt/fp-fakta-felles": "1.0.0",
    "@navikt/fp-kodeverk": "1.0.0",
    "@navikt/fp-storybook-utils": "1.0.0",
    "@navikt/fp-types": "1.0.0",
    "@navikt/fp-types-avklar-aksjonspunkter": "1.0.0",
    "@navikt/ft-form-hooks": "4.1.7",
    "@navikt/ft-form-validators": "2.0.39",
    "@navikt/ft-kodeverk": "2.1.19",
    "@navikt/ft-types": "2.1.21",
    "@navikt/ft-ui-komponenter": "2.0.42",
    "@navikt/ft-utils": "2.0.38",
    "classnames": "2.3.2",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-hook-form": "7.45.2",
    "react-intl": "6.4.4"
  },
  "devDependencies": {
    "@navikt/fp-storybook-utils": "1.0.0",
    "@navikt/fp-utils-test": "1.0.0",
    "@storybook/addon-actions": "7.1.1",
    "@storybook/react": "7.1.1",
    "@storybook/react-vite": "7.1.1",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.46.0",
    "storybook": "7.1.1",
    "stylelint": "15.10.2",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vitest": "0.33.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
