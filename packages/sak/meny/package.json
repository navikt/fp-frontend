{
  "name": "@navikt/fp-sak-meny",
  "description": "Meny som viser diverse handlinger en kan utføre på en fagsak",
  "license": "MIT",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./index.ts"
  },
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "clean": "rm -rf ./dist ./node_modules ./coverage ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7021"
  },
  "dependencies": {
    "@navikt/ds-css": "7.23.2",
    "@navikt/ds-react": "7.23.2",
    "@navikt/ft-form-hooks": "6.3.13",
    "@navikt/ft-form-validators": "3.2.5",
    "@navikt/ft-plattform-komponenter": "9.1.4",
    "@navikt/ft-ui-komponenter": "4.5.1",
    "@navikt/ft-utils": "3.6.3",
    "react": "19.1.0",
    "react-hook-form": "7.58.1",
    "react-intl": "7.1.11"
  },
  "devDependencies": {
    "@storybook/react": "9.0.13",
    "@storybook/react-vite": "9.0.13",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "eslint": "9.29.0",
    "storybook": "9.0.13",
    "stylelint": "16.21.0",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.2.4"
  }
}
