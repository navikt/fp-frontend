{
  "name": "@navikt/fp-sak-risikoklassifisering",
  "description": "Viser risikoklassifisering for valgt behandling. Viser en av tre komponenter avhengig av: Om ingen klassifisering er utført, om klassifisering er utført og ingen faresignaler er funnet og om klassifisering er utført og faresignaler er funnet",
  "license": "MIT",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./index.ts"
  },
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "clean": "rm -rf ./dist ./node_modules ./coverage ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7019"
  },
  "dependencies": {
    "@navikt/aksel-icons": "7.18.0",
    "@navikt/ds-css": "7.18.0",
    "@navikt/ds-react": "7.18.0",
    "@navikt/fp-kodeverk": "workspace:^",
    "@navikt/fp-types": "workspace:^",
    "@navikt/fp-types-avklar-aksjonspunkter": "workspace:^",
    "@navikt/ft-form-hooks": "6.2.10",
    "@navikt/ft-form-validators": "3.1.12",
    "@navikt/ft-ui-komponenter": "4.0.1",
    "@navikt/ft-utils": "3.3.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.56.0",
    "react-intl": "7.1.11"
  },
  "devDependencies": {
    "@navikt/fp-storybook-utils": "workspace:*",
    "@storybook/addon-actions": "8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-vite": "8.6.12",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "eslint": "9.24.0",
    "storybook": "8.6.12",
    "stylelint": "16.18.0",
    "typescript": "5.8.3",
    "vite": "6.2.5",
    "vitest": "3.1.1"
  }
}
