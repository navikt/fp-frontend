{
  "name": "@navikt/fp-sak-infosider",
  "description": "Diverse info- og feil-sider",
  "license": "MIT",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./index.ts"
  },
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "clean": "rm -rf ./dist ./node_modules ./coverage ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7020"
  },
  "dependencies": {
    "@navikt/ds-css": "7.21.1",
    "@navikt/ds-react": "7.21.1",
    "@navikt/ft-utils": "3.6.0",
    "react": "19.1.0",
    "react-intl": "7.1.11"
  },
  "devDependencies": {
    "@storybook/addon-actions": "8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-vite": "8.6.12",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "eslint": "9.26.0",
    "storybook": "8.6.12",
    "stylelint": "16.19.1",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.1.3"
