{
  "name": "@navikt/fp-storybook-utils",
  "version": "1.0.0",
  "module": "index.jsx",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "tsc": "tsc --pretty",
    "eslint": "eslint \"decorators/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"decorators/**/*.ts*\" --color",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/ft-form-hooks": "4.0.0",
    "history": "5.3.0",
    "react": "18.2.0",
    "react-hook-form": "7.43.2",
    "react-intl": "6.2.8",
    "react-router-dom": "6.8.1"
  },
  "exports": {
    ".": "./dist/index.js"
  }
}
