{
  "name": "@navikt/fp-storybook-utils",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "tsc": "tsc --pretty",
    "eslint": "eslint \"decorators/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"decorators/**/*.ts*\" --color",
    "prettier": "prettier --write decorators",
    "dev": "vite build --mode development --watch",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "dependencies": {
    "@navikt/ft-form-hooks": "4.0.20",
    "history": "5.3.0",
    "react": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-intl": "6.4.1",
    "react-router-dom": "6.10.0"
  },
  "devDependencies": {
    "eslint": "8.39.0",
    "typescript": "5.0.4",
    "vite": "4.2.1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  }
}
