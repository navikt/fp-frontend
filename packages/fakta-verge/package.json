{
  "name": "@navikt/fp-fakta-verge",
  "version": "1.0.0",
  "module": "index.ts",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.less\"",
    "dev": "webpack --env watch=true --config webpack.config.js",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "start-storybook --quiet -p 7017"
  },
  "dependencies": {
    "@navikt/fp-fakta-felles": "1.0.0",
    "@navikt/fp-kodeverk": "1.0.0",
    "@navikt/fp-types": "1.0.0",
    "@navikt/fp-types-avklar-aksjonspunkter": "1.0.0",
    "@navikt/ft-form-hooks": "4.0.0",
    "@navikt/ft-form-validators": "2.0.0",
    "@navikt/ft-kodeverk": "2.0.0",
    "@navikt/ft-types": "2.0.0",
    "@navikt/ft-ui-komponenter": "2.0.0",
    "@navikt/ft-utils": "2.0.0",
    "react": "18.2.0",
    "react-hook-form": "7.43.2",
    "react-intl": "6.2.8"
  },
  "devDependencies": {
    "@navikt/ds-css": "2.1.7",
    "@navikt/fp-storybook-utils": "1.0.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}
