  "dependencies": {
    "@sentry/browser": "7.41.0",
    "axios": "1.3.4"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.21.2",
    "cross-env": "7.0.3",
    "eslint": "8.35.0",
    "sinon": "15.0.1",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
