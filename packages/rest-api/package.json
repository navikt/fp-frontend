{
  "name": "@navikt/fp-rest-api",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
