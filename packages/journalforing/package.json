{
  "name": "@navikt/fp-journalforing",
  "version": "1.0.0",
  "module": "index.ts",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "dev": "cross-env node dev/webpack.dev.js",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7020"
  },
  "dependencies": {
    "dayjs": "1.11.7",
    "@navikt/ds-icons": "2.1.7",
    "@navikt/ds-react": "2.1.7",
    "@navikt/fp-assets": "1.0.0",
    "@navikt/fp-kodeverk": "1.0.0",
    "@navikt/fp-rest-api": "1.0.0",
    "@navikt/fp-rest-api-hooks": "1.0.0",
    "@navikt/fp-types": "1.0.0",
    "@navikt/ft-form-validators": "1.0.50",
    "@navikt/ft-kodeverk": "1.0.26",
    "@navikt/ft-plattform-komponenter": "1.4.11",
    "@navikt/ft-ui-komponenter": "1.3.13",
    "@navikt/ft-utils": "1.1.18",
    "pdfjs-dist": "2.12.313",  
    "react": "18.2.0",
    "react-intl": "6.2.8",
    "react-pdf": "5.7.0"
  },
  "devDependencies": {
    "@navikt/fp-storybook-utils": "1.0.0",
    "@navikt/fp-utils-test": "1.0.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "13.4.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  }
}
