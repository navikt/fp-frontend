{
  "name": "@navikt/fp-journalforing",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "description": "Komponent som lar saksbehandler journalføre (knytte til sak) journalposter som ikke har latt seg journalføres automatisk",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "dev": "vite build --mode development --watch",
    "build": "vite build",
    "clean": "rm -rf ./dist ./node_modules ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7019"
  },
  "dependencies": {
    "@navikt/aksel-icons": "3.4.2",
    "@navikt/ds-react": "3.4.2",
    "@navikt/ds-react-internal": "3.4.2",
    "@navikt/fp-kodeverk": "1.0.0",
    "@navikt/fp-rest-api": "1.0.0",
    "@navikt/fp-rest-api-hooks": "1.0.0",
    "@navikt/fp-types": "1.0.0",
    "@navikt/ft-form-hooks": "4.0.33",
    "@navikt/ft-form-validators": "2.0.28",
    "@navikt/ft-kodeverk": "2.1.9",
    "@navikt/ft-ui-komponenter": "2.0.31",
    "@navikt/ft-utils": "2.0.27",
    "dayjs": "1.11.7",
    "react": "18.2.0",
    "react-hook-form": "7.44.2",
    "react-intl": "6.4.2",
    "react-pdf": "7.0.2"
  },
  "devDependencies": {
    "@navikt/ds-css": "3.4.2",
    "@navikt/fp-storybook-utils": "1.0.0",
    "@navikt/fp-utils-test": "1.0.0",
    "@storybook/addon-actions": "7.0.18",
    "@storybook/react": "7.0.18",
    "@storybook/react-vite": "7.0.18",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "cross-env": "7.0.3",
    "eslint": "8.41.0",
    "storybook": "7.0.18",
    "stylelint": "15.6.2",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vitest": "0.31.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  }
}
