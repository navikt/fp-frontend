{
  "name": "@navikt/fp-prosess-simulering",
  "license": "MIT",
  "description": "Prosesspanel for resultat av simulering mot økonomi, lar saksbehandler vurdere etterbetaling til søker og tilbakekreving fra søker.",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./index.ts"
  },
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "clean": "rm -rf ./dist ./node_modules ./coverage ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7033"
  },
  "dependencies": {
    "@navikt/aksel-icons": "7.24.0",
    "@navikt/ds-react": "7.24.0",
    "@navikt/fp-kodeverk": "workspace:*",
    "@navikt/fp-types": "workspace:*",
    "@navikt/fp-types-avklar-aksjonspunkter": "workspace:*",
    "@navikt/fp-utils": "workspace:*",
    "@navikt/ft-form-hooks": "8.0.0",
    "@navikt/ft-form-validators": "4.0.2",
    "@navikt/ft-ui-komponenter": "4.5.3",
    "@navikt/ft-utils": "3.6.5",
    "classnames": "2.5.1",
    "dayjs": "1.11.13",
    "react": "19.1.0",
    "react-hook-form": "7.60.0",
    "react-intl": "7.1.11"
  },
  "devDependencies": {
    "@navikt/ds-css": "7.24.0",
    "@navikt/fp-config-eslint": "workspace:*",
    "@navikt/fp-config-typescript": "workspace:*",
    "@navikt/fp-config-vite": "workspace:*",
    "@navikt/fp-storybook-utils": "workspace:*",
    "@storybook/react": "9.0.15",
    "@storybook/react-vite": "9.0.15",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "cross-env": "7.0.3",
    "eslint": "9.30.1",
    "storybook": "9.0.15",
    "stylelint": "16.21.1",
    "typescript": "5.8.3",
    "vite": "7.0.2",
    "vitest": "3.2.4"
  }
