{
  "name": "@navikt/fp-sak-meny",
  "description": "Meny som viser diverse handlinger en kan utføre på en fagsak",
  "license": "MIT",
  "private": true,
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "clean": "rm -rf ./dist ./node_modules ./coverage ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7021"
  },
  "dependencies": {
    "@navikt/ds-css": "7.9.2",
    "@navikt/ds-react": "7.9.2",
    "@navikt/ft-form-hooks": "6.0.0",
    "@navikt/ft-form-validators": "3.0.0",
    "@navikt/ft-plattform-komponenter": "7.0.0",
    "@navikt/ft-ui-komponenter": "3.0.0",
    "@navikt/ft-utils": "3.0.0",
    "react": "19.0.0",
    "react-hook-form": "7.54.2",
    "react-intl": "7.1.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "8.5.2",
    "@storybook/react": "8.5.2",
    "@storybook/react-vite": "8.5.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "eslint": "9.19.0",
    "storybook": "8.5.2",
    "stylelint": "16.14.0",
    "typescript": "5.7.3",
    "vite": "6.0.11",
    "vitest": "3.0.4"
  }
}
