{
  "name": "@navikt/fp-sak-meny",
  "description": "Meny som viser diverse handlinger en kan utføre på en fagsak",
  "license": "MIT",
  "private": true,
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch=true",
    "test:coverage": "vitest --coverage",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\" --color",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\" --color",
    "stylelint": "stylelint \"src/**/*.module.css\"",
    "prettier": "prettier --write src",
    "clean": "rm -rf ./dist ./node_modules ./coverage ./.storybook-static-build",
    "build-storybook": "storybook build -o .storybook-static-build",
    "storybook": "storybook dev --quiet -p 7021"
  },
  "dependencies": {
    "@navikt/ds-css": "7.5.3",
    "@navikt/ds-react": "7.5.3",
    "@navikt/ft-form-hooks": "5.2.6",
    "@navikt/ft-form-validators": "2.5.4",
    "@navikt/ft-plattform-komponenter": "5.1.2",
    "@navikt/ft-ui-komponenter": "2.6.2",
    "@navikt/ft-utils": "2.5.1",
    "react": "18.3.1",
    "react-hook-form": "7.54.0",
    "react-intl": "6.8.7"
  },
  "devDependencies": {
    "@storybook/addon-actions": "8.4.2",
    "@storybook/react": "8.4.2",
    "@storybook/react-vite": "8.4.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "eslint": "9.14.0",
    "storybook": "8.4.2",
    "stylelint": "16.10.0",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vitest": "2.1.4"
  }
}
