{
  "name": "@navikt/ft-sak-infosider",
  "description": "Diverse info- og feil-sider",
  "version": "1.0.2",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "package.json"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "tsc": "tsc --pretty",
    "eslint": "eslint \"src/**/*.ts*\"",
    "eslint:fix": "eslint --fix \"src/**/*.ts*\"",
    "less:lint": "stylelint \"src/**/*.less\"",
    "build-webpack": "cross-env webpack --config webpack.config.js",
    "build-ts-declarations": "tsc index.ts ../../@types/externals.d.ts --declaration --emitDeclarationOnly --downlevelIteration --resolveJsonModule --jsx react --outDir dist --declarationMap --esModuleInterop --skipLibCheck",
    "build": "yarn build-webpack && yarn build-ts-declarations",
    "clean": "rm -rf ./dist ./node_modules",
    "storybook": "cross-env start-storybook -p 7020"
  },
  "dependencies": {
    "@navikt/ft-utils": "^1.0.31",
    "nav-frontend-alertstriper": "4.0.2",
    "nav-frontend-alertstriper-style": "3.0.2",
    "nav-frontend-typografi": "4.0.2",
    "nav-frontend-typografi-style": "2.0.2",
    "react": "17.0.2",
    "react-intl": "6.0.5"
  },
  "peerDependencies": {
    "@navikt/ft-utils": "1.0.30",
    "nav-frontend-alertstriper": "4.0.2",
    "nav-frontend-alertstriper-style": "3.0.2",
    "nav-frontend-typografi": "4.0.2",
    "nav-frontend-typografi-style": "2.0.2",
    "react": "17.0.2",
    "react-intl": "6.0.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.5.10",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/ft-frontend-saksbehandling"
  },
  "exports": {
    ".": "./dist/index.js",
    "./dist/style.css": "./dist/style.css"
  },
