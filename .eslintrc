{
  "root":true,
  "env":{
    "es6":true,
    "browser":true,
    "mocha":true
  },
  "extends":"airbnb",
  "parserOptions":{
    "sourceType":"module",
    "ecmaFeatures":{
      "ecmaVersion":8,
      "jsx":true,
      "impliedStrict":true
    }
  },
  "rules":{
    "linebreak-style":0,
    "jsx-a11y/no-static-element-interactions":0,
    "import/no-extraneous-dependencies": ["error", { "peerDependencies": true }],
    "import/no-named-as-default":0,
    "max-len":[
      2,
      160
    ],
    "react/no-typos":0,
    "react/jsx-closing-tag-location":0,
    "jsx-a11y/anchor-is-valid":0,
    "object-curly-newline":0,
    "react/no-unused-prop-types":0,
    "no-debugger":0,
    "no-console":"off"
  },
  "settings":{
    "import/resolver":{
      "webpack":{
        "config":"C:/git/fpsak-frontend/webpack/webpack.dev.js"
      }
    }
  }
}
